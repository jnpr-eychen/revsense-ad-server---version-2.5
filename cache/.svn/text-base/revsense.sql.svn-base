CREATE TABLE adrev_ad_types (id int(11) NOT NULL auto_increment, name varchar(64) NOT NULL default '', template text, PRIMARY KEY  (id), KEY name (name)) CHARACTER SET utf8 COLLATE utf8_bin 
CREATE TABLE adrev_ads (id int(11) NOT NULL auto_increment, userid int(11) NOT NULL default '0', zid varchar(13) NOT NULL default '', date int(11) NOT NULL default '0', status int(11) NOT NULL default '2', zone int(11) NOT NULL default '0', daypart_days int(11) default '0', daypart_hours int(11) default '0', startdate int(11) NOT NULL default '0', expires int(11) default '0', title varchar(255) default NULL, description text, display_url varchar(255) default NULL, url text, image text, email varchar(128) default NULL, phone varchar(64) default NULL, fax varchar(64) default NULL, content text, custom1 varchar(255) default NULL, custom2 varchar(255) default NULL, custom3 varchar(255) default NULL, custom4 varchar(255) default NULL, custom5 varchar(255) default NULL, custom6 varchar(255) default NULL, bid decimal(10,2) default '0.00', units int(11) NOT NULL default '0', total_units int(11) NOT NULL default '0', PRIMARY KEY  (id), KEY userid (userid,zid,status,zone), KEY startdate (startdate)) CHARACTER SET utf8 COLLATE utf8_bin 
CREATE TABLE adrev_aff_traffic (id int(11) NOT NULL auto_increment, date int(11) NOT NULL default '0', affid int(11) NOT NULL default '0', adtype varchar(5) default NULL, adid int(11) NOT NULL default '0', ip varchar(15) default NULL, referer text NOT NULL, amount decimal(10,3) NOT NULL default '0.000', PRIMARY KEY  (id), KEY date (date,affid,adid), KEY ip (ip), KEY affid (affid)) CHARACTER SET utf8 COLLATE utf8_bin  
CREATE TABLE adrev_keyword_map (id int(11) NOT NULL auto_increment, zoneid int(11) NOT NULL default '0', adid int(11) NOT NULL default '0', keywordid int(11) NOT NULL default '0', bid decimal(10,2) NOT NULL default '0.00', url text, PRIMARY KEY  (id), KEY adid (adid,keywordid), KEY bid (bid), KEY zoneid (zoneid)) CHARACTER SET utf8 COLLATE utf8_bin 
CREATE TABLE adrev_keywords (id int(11) NOT NULL auto_increment, keyword varchar(255) NOT NULL default '', fuzzy_keyword varchar(255) default NULL, mincpc double(10,2) NOT NULL default '0.00', PRIMARY KEY  (id), KEY keyword (keyword)) CHARACTER SET utf8 COLLATE utf8_bin 
CREATE TABLE adrev_payments (id int(11) NOT NULL auto_increment, date int(11) NOT NULL default '0', userid int(11) NOT NULL default '0', description varchar(255) default NULL, amount decimal(10,2) NOT NULL default '0.00', PRIMARY KEY  (id), KEY date (date,userid)) CHARACTER SET utf8 COLLATE utf8_bin 
CREATE TABLE adrev_settings (name varchar(32) NOT NULL default '', value text, PRIMARY KEY  (name)) CHARACTER SET utf8 COLLATE utf8_bin 
CREATE TABLE adrev_tokens (id int(11) NOT NULL auto_increment, userid int(11) NOT NULL default '0', token varchar(64) NOT NULL default '', date int(11) NOT NULL default '0', amount decimal(10,2) NOT NULL default '0.00', txid varchar(128) NOT NULL default '', txndate int(11) NOT NULL default '0', status varchar(255) NOT NULL default 'Pending', PRIMARY KEY  (id), KEY userid (userid,token)) CHARACTER SET utf8 COLLATE utf8_bin 
CREATE TABLE adrev_traffic (id int(11) NOT NULL auto_increment, userid int(11) NOT NULL default '0', date date NOT NULL default '0000-00-00', adid int(11) NOT NULL default '0', keywordid tinyint(4) NOT NULL default '0', clicks int(11) NOT NULL default '0', impressions int(11) NOT NULL default '0', orders int(11) NOT NULL default '0', amount decimal(10,3) NOT NULL default '0.000', PRIMARY KEY  (id), KEY userid (userid,date), KEY adid (adid), KEY date (date)) CHARACTER SET utf8 COLLATE utf8_bin 
CREATE TABLE adrev_users (id int(11) NOT NULL auto_increment, admin int(11) NOT NULL default '0', zid varchar(13) NOT NULL default '', date int(11) NOT NULL default '0', ip varchar(15) default NULL, status int(11) NOT NULL default '0', email varchar(128) NOT NULL default '', password varchar(32) NOT NULL default '', name varchar(128) default NULL, organization varchar(255) default NULL, country char(2) default NULL, street varchar(255) default NULL, city varchar(255) default NULL, state varchar(255) default NULL, postalcode varchar(64) default NULL, url varchar(255) default NULL, balance decimal(10,2) NOT NULL default '0.00', balance_update int(11) default '0', ref varchar(255) default NULL, lang varchar(10) DEFAULT 'EN', PRIMARY KEY  (id), KEY balance (balance), KEY date (date), KEY zid (zid), KEY email (email), KEY organization (organization), KEY name (name), KEY url (url)) CHARACTER SET utf8 COLLATE utf8_bin 
CREATE TABLE adrev_zones (id int(11) NOT NULL auto_increment, name varchar(64) default NULL, description varchar(255) default NULL, rtype int(11) default '1', style int(11) default '0', status int(11) default '1', auto_approve int(11) default '1', keywords_enable int(11) default '0', keywords_fuzzy int(11) NOT NULL default '0', keywords text, keywords_max int(11) NOT NULL default '0', daypart_enable int(11) default '0', daypart_days int(10) unsigned default '0', daypart_hours int(10) unsigned default '0', default_ad int(11) default NULL, max_zone_ads int(11) default '0', max_display_ads int(11) default '1', ad_sort varchar(32) default 'desc', urls_hide int(11) default '0', urls_tracking int(11) default '1', urls_target varchar(32) default NULL, ad_format text, rows int(11) default '1', cols int(11) default '1', layout_template text, template text, rate_type varchar(5) NOT NULL default 'CPC', rate decimal(10,2) NOT NULL default '0.00', aff_percent decimal(10,2) NOT NULL default '0.00', runtime int(11) NOT NULL default '0', units int(11) default '0', PRIMARY KEY  (id), KEY style (style,status)) CHARACTER SET utf8 COLLATE utf8_bin
